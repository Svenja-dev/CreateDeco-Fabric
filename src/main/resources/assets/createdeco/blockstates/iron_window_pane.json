{
  "multipart": [
    {
      "apply": {
        "model": "createdeco:block/iron_window_pane_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "false",
        "east": "false",
        "south": "false",
        "west": "false"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_noside"
      }
    },
    {
      "when": {
        "north": "true",
        "east": "false",
        "south": "false",
        "west": "false"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_noside_alt"
      }
    },
    {
      "when": {
        "north": "false",
        "east": "true",
        "south": "false",
        "west": "false"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false",
        "east": "false",
        "south": "true",
        "west": "false"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_noside_alt",
        "y": 180
      }
    },
    {
      "when": {
        "north": "false",
        "east": "false",
        "south": "false",
        "west": "true"
      },
      "apply": {
        "model": "createdeco:block/iron_window_pane_noside_alt",
        "y": 270
      }
    }
  ]
}
